<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


    <servlet>
        <servlet-name>GenerateDatabaseServlet</servlet-name>
        <servlet-class>com.danekja.edu.ormprobe.servlets.GenerateDatabaseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DaoSwitcherServlet</servlet-name>
        <servlet-class>com.danekja.edu.ormprobe.servlets.DaoSwitcherServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MenuRedirectServlet</servlet-name>
        <servlet-class>com.danekja.edu.ormprobe.servlets.MenuRedirectServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>GenerateDatabaseServlet</servlet-name>
        <url-pattern>/generate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DaoSwitcherServlet</servlet-name>
        <url-pattern>/switch</url-pattern>
    </servlet-mapping><servlet-mapping>
        <servlet-name>MenuRedirectServlet</servlet-name>
        <url-pattern>/menu</url-pattern>
    </servlet-mapping>


</web-app>
